extends layout

block layout-content
  div.View.WelcomeView
    h1.Banner WHATABYTE

    div.Message
      div.Title
        h3 Making the Best
        h1 Food For Devs
        h1#year  

        script.
            document.getElementById('year').textContent = pugs('cute');

      span.Details Access the WHATABYTE Team Portal
    div.NavButtons
      a(href="/user")
          div.NavButton Just dive in!

    div#cy

    script. 
        var cy = cytoscape({
        container: document.getElementById('cy'),
        elements: [
            { data: { id: 'a' } },
            { data: { id: 'b' } },
            {
            data: {
                id: 'ab',
                source: 'a',
                target: 'b'
            }
            }]
        });